@{
    Layout = null;
    var newsbyid = (List<Model.EntityFramework.BAIDANG>)ViewBag._k_news;
    var anhdaidien = "";
    if (newsbyid != null && newsbyid.Count > 0)
    {
        anhdaidien = newsbyid.FirstOrDefault().AnhDaiDien;
    }
}

